				icon.id = icon.name+'-iconBack';
				icon.tag = "icon";
				icon.E = null;
				icon.pos = {
					x: cl,
					y: cly
				};
				spc.addActor(icon);
				icon.card = this;
				
				cl++; 
				lx += a.padX;
				cl >= b && (ly += a.padY, lx = a.offsetX, cl = 0, cly++);
			}
			
			listBox.getIconFromPos = function(x,y, actor) {
