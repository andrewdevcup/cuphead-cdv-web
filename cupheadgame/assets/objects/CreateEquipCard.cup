		  	this.visible = true;
		  	this._oy = 50;
		  	this.playAnim('icon_sheen');
		  }
		  pIconSel.addActor(pIconSheen);
		  
		  //Equiped marker
		  var pIconEq = new b5.Actor();
		  pIconEq.atlas = pIconSheen.atlas;
		  pIconEq.visible = !pos.unequiped;
		  pIconEq.playAnim('icon_equiped',true);
