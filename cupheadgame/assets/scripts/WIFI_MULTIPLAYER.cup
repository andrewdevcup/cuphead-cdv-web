b5.Utils.getRandomCode = function(length) {
	var str = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ";
	for(var i = 0, c = ""; i < length; i++) {
		var n = str[Math.round(Math.random()*(str.length-1))];
		n = Math.random() > 0.5 ? n.toLowerCase() : n;
		c += n;
	}
	return c;
};

