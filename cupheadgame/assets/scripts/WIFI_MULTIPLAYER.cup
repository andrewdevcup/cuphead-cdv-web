			var player2 = sceneMain.data.players.player2;
			sceneMain.data.removePlayer(player2);
		}
		b5.Game.Input.reset();
	}
	
	this.isGuest = false;
	this.isHost = false;
	
	//Clear room in database
	try{b5.MPDB.ref(this.data.HOST_CODE).remove()}catch(e){}
